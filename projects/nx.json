{
  "npmScope": "projects",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "dou-coding": {
      "tags": []
    },
    "dou-coding-e2e": {
      "tags": [],
      "implicitDependencies": ["dou-coding"]
    },
    "projects-api": {
      "tags": []
    },
    "projects-ui": {
      "tags": []
    },
    "projects-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["projects-ui"]
    }
  }
}
